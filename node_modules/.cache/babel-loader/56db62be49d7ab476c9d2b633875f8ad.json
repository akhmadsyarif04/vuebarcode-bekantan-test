n\n        };\n        db.collection('projects').add(project).then(function () {\n          console.log('added to db');\n          _this.loading = false;\n          _this.dialog = false;\n\n          _this.$emit('projectAdded'); // komunikasi ke file navbar untuk snackbar\n\n        });\n      }\n    }\n  },\n  computed: {\n    formattedDate: function formattedDate() {\n      // untuk format date silahkan lihat pada documentasi date-fns\n      return this.due ? format(this.due, 'Do MMM YYYY') : '';\n    }\n  }\n};",{"version":3,"sources":["Popup.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoCA;AACA;AACA,OAAA,MAAA,MAAA,iBAAA,C,CACA;AACA;;AACA,OAAA,EAAA,MAAA,MAAA;AAEA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,EADA;AAEA,MAAA,OAAA,EAAA,EAFA;AAGA,MAAA,GAAA,EAAA,IAHA;AAIA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,MAAA,IAAA,CAAA,IAAA,6BAAA;AAAA,OADA,CAJA;AAOA,MAAA,OAAA,EAAA,KAPA;AAQA,MAAA,MAAA,EAAA;AARA,KAAA;AAUA,GAZA;AAaA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AAAA;;AACA,UAAA,KAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA,EAAA;AACA,aAAA,OAAA,GAAA,IAAA;AAEA,YAAA,OAAA,GAAA;AACA,UAAA,KAAA,EAAA,KAAA,KADA;AAEA,UAAA,OAAA,EAAA,KAAA,OAFA;AAGA,UAAA,GAAA,EAAA,MAAA,CAAA,KAAA,GAAA,EAAA,aAAA,CAHA;AAIA,UAAA,MAAA,EAAA,QAJA;AAKA,UAAA,MAAA,EAAA,SALA,CAOA;;AAPA,SAAA;AAQA,QAAA,EAAA,CAAA,UAAA,CAAA,UAAA,EAAA,GAAA,CAAA,OAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,aAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,KAAA;;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,cAAA,EAJA,CAIA;;AACA,SALA;AAMA;AACA;AApBA,GAbA;AAmCA,EAAA,QAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA;AACA,aAAA,KAAA,GAAA,GAAA,MAAA,CAAA,KAAA,GAAA,EAAA,aAAA,CAAA,GAAA,EAAA;AACA;AAJA;AAnCA,CAAA","sourcesContent":["<template>\n    <!--\n        v-model=\"dialog\" untuk menutup modal dialog ketika proses add data selesai yang mana berhubungan dengan variabel dialog\n        jika false dia akan tertutup, jika true dia akan muncul\n    -->\n<v-dialog max-width=\"600px\" v-model=\"dialog\">\n    <v-btn flat slot=\"activator\" color=\"success\">Add New Project</v-btn>\n    <v-card>\n        <v-card-title>\n            <h2>Add a New Project</h2>\n        </v-card-title>\n        <v-card-text>\n            <v-form class=\"px-3\" ref=\"form\">\n                <v-text-field name=\"name\" label=\"Title\" id=\"id\" v-model=\"title\" prepend-icon=\"folder\" :rules=\"inputRules\"></v-text-field>\n                <v-textarea label=\"Information\" name=\"name\" v-model=\"content\" prepend-icon=\"edit\" :rules=\"inputRules\"></v-textarea>\n\n                <v-menu>\n                    <v-text-field :rules=\"inputRules\" :value=\"formattedDate\" slot=\"activator\" label=\"Due date\" prepend-icon=\"date_range\" id=\"id\"></v-text-field>\n                    <v-date-picker v-model=\"due\"   ></v-date-picker>\n                </v-menu>\n\n                <!-- untuk jarak  -->\n                <v-spacer></v-spacer>\n                <!--\n                    :loading untuk button loading ketikda proses masih berjalan.\n                    nilai nya hanya true dan false\n                -->\n                <v-btn color=\"success mx-0 mt-3\" @click=\"submit\" :loading=\"loading\">Add a Project</v-btn>\n            </v-form>\n        </v-card-text>\n\n    </v-card>\n</v-dialog>\n</template>\n\n<script>\n// untuk mengubah format deafult date pada vuetify\n// install terlebih dulu npm install date-fns\nimport format from 'date-fns/format'\n// import db firebase\n// @ adalah root pada /src\nimport db from '@/fb'\n\nexport default {\n    data() {\n        return {\n            title: '',\n            content: '',\n            due: null,\n            inputRules: [\n                v => v.length >= 3 || 'Minimun length 3 characters'\n            ],\n            loading: false,\n            dialog: false,\n        }\n    },\n    methods: {\n        submit() {\n            if(this.$refs.form.validate()){\n                this.loading = true;\n\n                const project = {\n                    title: this.title,\n                    content: this.content,\n                    due: format(this.due, 'Do MMM YYYY'),\n                    person: 'Killua',\n                    status: 'Ongoing',\n                }\n                // insert data ke firebase melalui file fb.js yang di import diatas\n                db.collection('projects').add(project).then(() => {\n                    console.log('added to db');\n                    this.loading = false;\n                    this.dialog = false;\n                    this.$emit('projectAdded'); // komunikasi ke file navbar untuk snackbar\n                })\n            }\n        }\n    },\n    computed: {\n        formattedDate(){\n            // untuk format date silahkan lihat pada documentasi date-fns\n            return this.due ? format(this.due, 'Do MMM YYYY') : ''\n        }\n    }\n}\n</script>\n\n<style lang=\"css\" scoped>\n</style>\n"],"sourceRoot":"src/components"}]}